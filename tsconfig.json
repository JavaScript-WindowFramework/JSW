{
	"compilerOptions": {
		"sourceMap": true,
		"target": "es5",
		"module": "commonjs",
		"outDir": "./dist/js",
		"rootDir": "./src/",
		"declaration": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"lib": [
			"es2015",
			"dom"
		]
	},
	"typedocOptions":{
		"name": "JSW",
		"json": "doc/document.json",
		"mode": "file",
		"includeDeclarations": false,
		"hideGenerator": false
	}
}